<style>
  .image-text {position: relative; text-align: left;}
  .image-text .text-layer .container {height: 100%;}
  .image-text .text-layer .container .grid-full {height: 100%;}
  .image-text .title {color: inherit; max-width: 10em; letter-spacing: 0.02em; margin-bottom: 0.5em;}
  .image-text .img {width: 100%; background-size: cover; background-position: center; background-repeat: no-repeat;}
  .image-text p {color: inherit; }
  .image-text p:last-child {margin: 0;}
  .image-text .captions {padding: 0 1.5em;}
  .image-text .btn {margin-top: 2.5em; white-space: nowrap; min-width: 13em;}
  .image-text p a {text-decoration: underline;}
  
  .text-text {position: relative; text-align: left;}
  .text-text .title {color: inherit; margin-bottom: 0; letter-spacing: 0.02em;}
  .text-text .title.margin {margin-bottom: 0.5em;}
  .text-text p {color: inherit; }
  .text-text p:last-child {margin: 0;}
  .text-text .captions {padding: 0 1.5em; margin: auto;}
  .text-text .btn {margin-top: 1.3em; white-space: nowrap; min-width: 13em;}
  .text-text p a {text-decoration: underline;}
  
  .custom-blocks {text-align: center;}
  .custom-blocks .container {padding: 0;}
  .custom-blocks .heading {padding: 0 1.5em; margin: 0 auto 4em;}
  .custom-blocks .title {letter-spacing: 0.02em;}
  .custom-blocks .grid {margin-left: -3em;}
  .custom-blocks .grid__item {margin-bottom: 2em; padding-left: 3em;}
  .custom-blocks .block-title {font-weight: 500; font-size: 1.11em;}
  .custom-blocks img {max-width: 100%; margin-bottom: 1em;}
  .custom-blocks p a {text-decoration: underline;}
  
  .image-text-logos {position: relative; text-align: left;}
  .image-text-logos .text-layer .container {height: 100%;}
  .image-text-logos .text-layer .container .grid-full {height: 100%;}
