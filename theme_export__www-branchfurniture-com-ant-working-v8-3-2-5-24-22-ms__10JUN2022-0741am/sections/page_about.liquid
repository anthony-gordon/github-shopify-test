  .image-text-logos .content {position: relative; z-index: 1;}
  .image-text-logos .img {width: 100%; height: 100%; background-size: cover; background-position: center left; background-repeat: no-repeat;}
  .image-text-logos p {color: inherit; }
  .image-text-logos p:last-child {margin: 0;}
  .image-text-logos .captions {padding: 0 1.5em;}
  .image-text-logos .btn {margin-top: 5em; white-space: nowrap; min-width: 13em;}
  .image-text-logos .logos {margin-top: 2em; width: 106%;}
  .image-text-logos .logos img {height: 4em; margin-right: 1.5em; margin-bottom: 1em;}
  .image-text-logos .logos img:last-child {margin-right: 0;}
  .image-text-logos p a {text-decoration: underline;}
  
  .our-prices {position: relative; text-align: left; overflow: hidden;}
  .our-prices .title {color: inherit; margin-bottom: 0; letter-spacing: 0.02em;}
  .our-prices .title.margin {margin-bottom: 0.5em;}
  .our-prices p {color: inherit; }
  .our-prices p:last-child {margin: 0;}
  .our-prices .captions {padding: 0 3em 0 0; margin: auto;}
  .our-prices .btn {margin-top: 1.3em; white-space: nowrap; min-width: 13em;}
  .our-prices .price .item {position: relative; text-align: center; padding-right: 0.3em;}
  .our-prices .price .item span {font-size: 1.11em; font-weight: 700; line-height: 120%; display: block; width: 100%; padding: 1em 1em 1.3em; height: 100%; color: {{settings.content_second_color}}; background: {{settings.content_third_color}}; display: -webkit-box !important; display: -moz-box !important; display: -ms-flexbox !important; display: -webkit-flex !important; display: flex !important; -webkit-box-pack: center;-ms-flex-pack: center;justify-content: center; -webkit-box-align: center;-ms-flex-align: center;align-items: center;}
  .our-prices .price .item:last-child {padding-right: 0;}
  .our-prices .price .block {margin-bottom: 3em;}
  .our-prices .price .block:last-child {margin-bottom: 0;}
  .our-prices .price .block .text {margin-bottom: 1em;}
  .our-prices .price .item.transparent span {background: transparent; color: {{settings.content_text_color}}; font-weight: 500; font-size: 1em;}
  .our-prices .price .item.inverted span {background: transparent; border: 2px solid; color: {{settings.content_third_color}};}
  .our-prices .price .item .note {position: absolute; font-size: 0.95em; line-height: 120%; white-space: nowrap; font-weight: 500; top: 100%; left: 50%; margin-top: 0.5em; -webkit-transform: translateX(-50%); transform: translateX(-50%);}
  
  .how-we-fixed-it {position: relative; overflow: hidden;}
  .how-we-fixed-it .title {color: inherit; margin-bottom: 0; letter-spacing: 0.02em;}
  .how-we-fixed-it .title.margin {margin-bottom: 0.5em;}
  .how-we-fixed-it p {color: inherit; }
  .how-we-fixed-it p:last-child {margin: 0;}
  .how-we-fixed-it .heading {margin-bottom: 5em; text-align: center;}
  .how-we-fixed-it .container {max-width: 57em;}
  .how-we-fixed-it .ways .way-title {font-size: 1.7em; font-weight: 700; margin-bottom: 2em;}
  .how-we-fixed-it .steps {position: relative;}
  .how-we-fixed-it .item {position: relative; width: 5em; margin-bottom: 4em; }
  .how-we-fixed-it .item:before {content: ''; position: absolute; top: 0; left: 0; width: 2em; height: 2em; font-size: 2.5em; background: #fff; border-radius: 50%;}
  .how-we-fixed-it .item .info {position: absolute; font-weight: 500; text-align: left; left: 90%; top: 0; white-space: nowrap;}
  .how-we-fixed-it .item .item-title {font-size: 1.7em; font-weight: 700; margin-bottom: 0.1em;}
  .how-we-fixed-it .item .item-description {font-size: 0.95em; line-height: 150%;}
  .how-we-fixed-it .item .number {position: relative;width: 2em; height: 2em; font-weight: 700; font-size: 2.5em; border-radius: 50%; background: #eeeeee; color: #666666; display: -webkit-box !important; display: -moz-box !important; display: -ms-flexbox !important; display: -webkit-flex !important; display: flex !important;-webkit-box-pack: center;-ms-flex-pack: center;justify-content: center; -webkit-box-align: center;-ms-flex-align: center;align-items: center;}
  .how-we-fixed-it img {width: 100%; max-width: 19em; display: block;}
  .how-we-fixed-it .mini-icon {width: 0.753em; margin-right: 0.5em;}
  .how-we-fixed-it .ways.new .item .number {background: {{settings.content_second_color}}; color: {{settings.content_text_color}};}
  .how-we-fixed-it .ways.old .item .info {color: #666;}
  .how-we-fixed-it .item .number img {max-width: 0.85em; }
  .how-we-fixed-it .ways .line {position: absolute; width: 4px; left: 2.5em; margin-left: -2px; background: #eee;}
  .how-we-fixed-it .ways.new .line {background: {{settings.content_main_color}};}
  .how-we-fixed-it .item .number, .how-we-fixed-it .item .info {-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=30)"; filter: alpha(opacity=30); -moz-opacity: 0.3; -khtml-opacity: 0.3; opacity: 0.3;}
  .how-we-fixed-it .item.appear .number, .how-we-fixed-it .item.appear .info {-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)"; filter: alpha(opacity=100); -moz-opacity: 1; -khtml-opacity: 1; opacity: 1;}
  .how-we-fixed-it img.relax {-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=30)"; filter: alpha(opacity=30); -moz-opacity: 0.3; -khtml-opacity: 0.3; opacity: 0.3;}
  .how-we-fixed-it img.relax.appear {-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)"; filter: alpha(opacity=100); -moz-opacity: 1; -khtml-opacity: 1; opacity: 1;}
  
  @media all and (min-width: 900px){
    .image-text-logos .image-wrap .img {position: absolute; top: 0; right: auto; width: 50%; height: 100%;}
    .image-text-logos .row-reverse .image-wrap .img {right: auto; left: 0;}
    .our-prices .one-third {width: 37%;}
    .our-prices .two-thirds {width: 63%;}
  }
  
  @media all and (max-width: 900px){
    .image-text {text-align: center;}
    .image-text .captions {margin: auto;}
    .image-text .container {padding: 0;}
    .image-text .btn {width: 100%;}
    .image-text .title {margin-left: auto; margin-right: auto;}
    
    .text-text {text-align: center;}
    
    .custom-blocks img {margin-bottom: 0;}
    .custom-blocks .block-title {font-size: 1em;}
    .custom-blocks .grid {display: block !important; margin: 0 auto !important; max-width: 37em;}
    .custom-blocks .grid .grid__item {padding: 0 !important; margin: 0;}  
    .custom-blocks .slick-dots {position: absolute; bottom: 1em; margin: 0;}
    .custom-blocks .slick-dots li button {background: #fff;} 
    .custom-blocks .btn {margin-top: 1.2em;}
    
    .image-text-logos {text-align: center;} 
    .image-text-logos .logos {margin-left: -3%;}
    .image-text-logos .captions {margin: auto;}
    .image-text-logos .logos img {height: 3em; margin: 0 0.8em 1em !important;}
    .image-text-logos .img {width: calc(100% + 3rem); margin-left: -1.5rem;}
    
    .our-prices .container {padding: 0;}
    .our-prices .captions {text-align: center; padding: 0 1.5rem; padding-bottom: 2.5em;}
    .our-prices .grid {margin: 0;}
    .our-prices .grid .grid__item {padding: 0;}
    .our-prices .price {padding: 0 1em !important; overflow-x: auto;}
    .our-prices .price .item span {font-size: 1em; padding: 0.6em 1em 0.8em;}
    .our-prices .price .block {min-width: 600px; padding-right: 1em; margin-bottom: 1.5em;}
    .our-prices .price .block .item.transparent {width: 42% !important;}
    .our-prices .price .item .note {font-size: 1em;}
    
    .how-we-fixed-it .heading {margin-bottom: 2em;}
    .how-we-fixed-it .ways {font-size: 0.8em;}
    .how-we-fixed-it .grid {margin: 0;}
    .how-we-fixed-it .grid .grid__item {padding: 0;}
    .how-we-fixed-it img.relax {margin: auto;}
    .how-we-fixed-it .steps {max-width: 31em; margin: auto;}
    .how-we-fixed-it .item {margin-bottom: 5em;}
    .how-we-fixed-it .ways.new .item {margin-bottom: 7em;}
    .how-we-fixed-it .item .info {white-space: initial; font-weight: 400; width: 75vw;}
    .how-we-fixed-it .container {padding: 0 1rem;}
    .how-we-fixed-it .ways .way-title  {text-align: center;}
    .how-we-fixed-it .item .item-description {font-size: 1.25em;}
    .how-we-fixed-it .item .item-title {margin-bottom: 0.3em;}
  }  
  
  
  @media all and (max-width: 600px){
    .text-text .container {padding: 0 1rem;}
    .custom-blocks .heading {padding: 0 1rem;}
    .custom-blocks .btn {max-width: 100%; display: block; margin: 1.2rem 1.2rem 0;}
    .image-text-logos .btn {width: 100%; margin-top: 2em;}
    .image-text-logos .container {padding: 0 1.2rem;}
    .image-text-logos .img {width: calc(100% + 2.4rem); margin-left: -1.2rem; background-position: center;}
  }
</style>

{%- for block in section.blocks -%}
{%- if block.type == "image_text" -%}
<section class="image-text" id="block-{{block.id}}">
  <style>
    #block-{{block.id}} {padding: {{block.settings.padding_desktop}}; }  
    #block-{{block.id}} .text-layer {padding: {{block.settings.padding_desktop}};}  
    {% if block.settings.image == blank %}#block-{{block.id}} .text-layer {background: {{block.settings.bg_color}};}{% endif %}
    #block-{{block.id}} .image-layer {background: {{block.settings.bg_color}};  }
    #block-{{block.id}} .img {padding-top: {{block.settings.image_height}}%;}
    #block-{{block.id}} .captions {max-width: {{block.settings.text_width}}em; padding: {{block.settings.text_padding_desktop}};}
    @media all and (min-width: 900px){
      {% if block.settings.image != blank %}#block-{{block.id}} .text-layer {position: absolute; top: 0; height: 100%; width: 100%; left: 0; }  {% endif %}
      #block-{{block.id}} .title {font-size: {{block.settings.title_size}};}
      #block-{{block.id}} .text {font-size: {{block.settings.text_size}};}
    }
    @media all and (max-width: 900px){
      #block-{{block.id}} {padding: {{block.settings.padding_mobile}};}
      #block-{{block.id}} .captions {padding: {{block.settings.text_padding_mobile}};}
      #block-{{block.id}} .img {padding-top: {{block.settings.image_height_mobile}}%;}
      #block-{{block.id}} .image-layer {padding: 0 1.5em; background: transparent; position: relative;}
      #block-{{block.id}} .image-layer:before {content: ''; background: {{block.settings.bg_color}}; display: block; position: absolute; bottom: 0; height: 50%; width: 100%; left: 0;}
      #block-{{block.id}} .image-layer .img {position: relative;}
      #block-{{block.id}} .text-layer {padding: 0; background: {{block.settings.bg_color}};} 
      #block-{{block.id}} .title {font-size: {{block.settings.title_size_mobile}};}
    }
  </style>
  {% unless block.settings.full_width %}<div class="container">{% endunless %}
  {%- if block.settings.image != blank -%}
    <div class="grid-full image-layer align-items-center {{block.settings.image_position}}">
      {%- if block.settings.image != blank -%}
      <div class="grid__item image one-half medium-down--one-whole">
        <div class="img lazyload" data-bgset="{% include 'lazy_src', image: block.settings.image %}"></div>
      </div>
      {%- endif -%}
      <div class="grid__item content {% if block.settings.image != blank %}one-half{% else %}one-whole{% endif %} medium-down--hide">
      </div>
    </div>
  {%- endif -%}
    <div class="text-layer">
      <div class="container">
        <div class="grid-full align-items-center {{block.settings.image_position}}">
          {%- if block.settings.image != blank -%}
          <div class="grid__item image one-half medium-down--hide">
          </div>
          {%- endif -%}
          <div class="grid__item content {% if block.settings.image != blank %}one-half{% else %}one-whole{% endif %} medium-down--one-whole">
            <div class="captions" style="color: {{block.settings.text_color}}">
              {%- if block.settings.title != "" -%}
              <{% if forloop.first %}h1{% else %}h2{% endif %} class="title">{{ block.settings.title }}</{% if forloop.first %}h1{% else %}h2{% endif %}>
              {%- endif -%}
              {%- if block.settings.text != "" -%}
              <div class="text">{{ block.settings.text }}</div>
              {%- endif -%}
              {%- if block.settings.button != "" -%}
              <a href="{{ block.settings.link }}" class="btn {{block.settings.button_style}}">
                {{ block.settings.button }}
              </a>
              {%- endif -%}
            </div>
          </div>
        </div>
      </div>
    </div>
  {% unless block.settings.full_width %}</div>{% endunless %}
</section>
{%- elsif block.type == "text_text" -%}
<section class="text-text" id="block-{{block.id}}">
  <style>
    #block-{{block.id}} {padding: {{block.settings.padding_desktop}};}
    #block-{{block.id}} .wrapper {background: {{block.settings.bg_color}};}
    #block-{{block.id}} .captions {padding: {{block.settings.text_padding_desktop}};color: {{block.settings.text_color}}}
    @media all and (min-width: 900px){
      #block-{{block.id}} .title {font-size: {{block.settings.title_size}};}
      #block-{{block.id}} .text {font-size: {{block.settings.text_size}};}
    }
    @media all and (max-width: 900px){
      #block-{{block.id}} {padding: {{block.settings.padding_mobile}};}
      #block-{{block.id}} .captions {padding: {{block.settings.text_padding_mobile}};}
      #block-{{block.id}} .grid__item:last-child .captions {padding-top: 0;} 
      #block-{{block.id}} .title {font-size: {{block.settings.title_size_mobile}};}
    }
  </style>
  {% unless block.settings.full_width %}<div class="container">{% endunless %}
  <div class="wrapper">
    <div class="{% if block.settings.full_width %}container{% endif %}">
      <div class="grid">
        <div class="grid__item {% if block.settings.title2 != "" or block.settings.text2 != "" or block.settings.button2 != "" %}one-half{% else %}one-whole{% endif %} medium-down--one-whole">
          <div class="captions">
            {%- if block.settings.title != "" -%}
            <h2 class="title {% if block.settings.text != "" %}margin{% endif %}">{{ block.settings.title }}</h2>
            {%- endif -%}
            {%- if block.settings.text != "" -%}
            <div class="text">{{ block.settings.text }}</div>
            {%- endif -%}
            {%- if block.settings.button != "" -%}
            <a href="{{ block.settings.link }}" class="btn {{block.settings.button_style}}">
              {{ block.settings.button }}
            </a>
            {%- endif -%}
          </div>
        </div>
        {%- if block.settings.title2 != "" or block.settings.text2 != "" or block.settings.button2 != ""  -%}
        <div class="grid__item one-half medium-down--one-whole">
          <div class="captions">
            {%- if block.settings.title2 != "" -%}
            <h2 class="title {% if block.settings.text2 != "" %}margin{% endif %}">{{ block.settings.title2 }}</h2>
            {%- endif -%}
            {%- if block.settings.text2 != "" -%}
            <div class="text">{{ block.settings.text2 }}</div>
            {%- endif -%}
            {%- if block.settings.button2 != "" -%}
            <a href="{{ block.settings.link2 }}" class="btn {{block.settings.button_style}}">
              {{ block.settings.button2 }}
            </a>
            {%- endif -%}
          </div>
        </div>
        {%- endif -%}
      </div>
    </div>
  </div>
  {% unless block.settings.full_width %}</div>{% endunless %}
</section>
{%- elsif block.type == "custom_blocks" -%}
<section class="custom-blocks" id="block-{{block.id}}">
  <style>
    #block-{{ block.id }} {background-color: {{block.settings.bg_color}}; padding: {{block.settings.padding_desktop}};}
    #block-{{ block.id }} .heading {max-width: {{block.settings.heading_width}}; }
    #block-{{ block.id }} img {height: {{block.settings.images_height}};}
    @media all and (min-width: 900px){
      #block-{{block.id}} .title {font-size: {{block.settings.title_size}};}
      #block-{{block.id}} .text {font-size: {{block.settings.text_size}};}
    }
    @media all and (max-width: 900px){
      #block-{{ block.id }} {padding: {{block.settings.padding_mobile}};}
      #block-{{ block.id }} .heading {margin: 0 auto 2em;}
      #block-{{block.id}} .title {font-size: {{block.settings.title_size_mobile}};}
    }
  </style>
  <div class="container">
    <div class="heading">
      {%- if block.settings.title != "" -%}
      <h2 class="title">{{block.settings.title}}</h2>
      {%- endif -%}
      {%- if block.settings.text != "" -%}
      <div class="text">{{block.settings.text}}</div>
      {%- endif -%}
    </div>
    <div class="grid medium-down--block justify-content-center">
      {%- for i in (1..6) -%}
      {%- assign image = 'image' | append: i -%}
      {%- assign title = 'title' | append: i -%}
      {%- assign button = 'button' | append: i -%}
      {%- assign link = 'link' | append: i -%}
      {%- if block.settings[title] != "" or block.settings[image] != blank -%}
      <div class="grid__item {{block.settings.items_per_row}} medium-down--one-whole">
        {% if block.settings[button] == "" and block.settings[link] != blank %}<a href="{{block.settings[link]}}">{% endif %}
        <div class="wrap">
          {%- if block.settings[image] != blank -%}
          <img class="lazyload" alt="" data-srcset="{% include 'lazy_src', image: block.settings[image] %}"/>
          {%- endif -%}
          {%- if block.settings[title] != "" -%}
          <div class="block-title">{{block.settings[title]}}</div>
          {%- endif -%}
          {%- if block.settings[button] != "" -%}
          <a href="{{block.settings[link]}}" class="btn">{{block.settings[button]}}</a>
          {%- endif -%}
        </div>
        {% if block.settings[button] == "" and block.settings[link] != blank %}</a>{% endif %}
      </div>
      {%- endif -%}
      {%- endfor -%}
    </div>
    {%- if block.settings.button != "" -%}
    <a href="{{ block.settings.link }}" class="btn">
      {{ block.settings.button }}
    </a>
    {%- endif -%}
  </div>
</section>
<script>
  $(function(){
     var checkCarousel = function(){
      if($(window).width() > 900){
        if($('#shopify-section-{{ section.id }} .custom-blocks .grid').hasClass('slick-initialized')){
           $('#shopify-section-{{ section.id }} .custom-blocks .grid').slick('unslick');
        }
  	  }else{
        if(!$('#shopify-section-{{ section.id }} .custom-blocks .grid').hasClass('slick-initialized')){
           $('#shopify-section-{{ section.id }} .custom-blocks .grid').slick({
              autoplay: false,
              infinite: true,
              slidesToShow: 1,
              pauseOnHover: true,
              slidesToScroll: 1,
              speed: 700,
              dots: true,
              arrows: false
            });
        }      
      }
    }
    checkCarousel();
    $(window).resize(function(){
      checkCarousel();
    });
  });
  </script>
