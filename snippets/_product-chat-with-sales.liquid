  }

  setTimeout(function(){
    $('.product-wrap .price').css('min-width', $('.product-wrap .price').width() + 5);
  },500);

  function ReadMore(existingId) {
    $('.product-wrap .description').each(function(){
      if($(this).children().length > 1){
        if (existingId && $(`#read-more-${existingId}`).length) {
          $(this).children(':first').show();
          $(`#read-more-${existingId}`).show();
        } else if (!$(this).children().find('.read-more').length) {
          var randomId = ~~(Math.random() * 1000);
          $(this).children(':first').append(`<a href="#" id="read-more-${randomId}" data-id="${randomId}" class="read-more">Read More</a>`).show();
        }
      }
    });
  }
  function ReadLess(existingId){
    $('.product-wrap .description').each(function(){
      if($(this).children().length > 1){
        if (existingId && $(`#read-less-${existingId}`).length) {
          $(this).children(':nth-last-child(1)').show();
          $(`#read-less-${existingId}`).show();
        } else {
          $(this).children(':nth-last-child(1)').append(`<a href="#" id="read-less-${existingId}" data-id="${existingId}" class="read-less">Read Less </a>`).show();
        }
      }
    });
  }

  //ReadMore();
  $(document).on('click', '.product-wrap .description .read-more', function(){
    $(this).closest('.description').children().show();
    $(this).hide();
    var existingId = $(this).attr("data-id")
    ReadLess(existingId);
    return false;
  });

  $(document).on('click', '.product-wrap .description .read-less', function(){
    $(this).closest('.description').children().hide();
    $(this).hide();
    var existingId = $(this).attr("data-id");
    ReadMore(existingId);
    return false;
  });
  
  // prevent mix of gallery image onclick interaction and ar button onclick interaction
  $('epigraph-ar').click(function(ev) {
      ev.preventDefault();
      ev.stopPropagation();
  });
  });
</script>

