  {% if settings.enable_mobile_sticky_atc %}
  <div id="sticky-atc" class="sticky-el">
    {% comment %}
    <form action="/cart/add" method="POST" class="variants add-to-cart-form" enctype="multipart/form-data" id="hlxn-atc-form">
    {% endcomment %}
      <input type="hidden" id="hlxn-product-{{product.id}}-variant" name="id[]" value="{{product.selected_or_first_available_variant.id}}" />
      <div class="sticky-grid">
        <div class="sticky-left-side">
          
         <div><h4 id="hlxn-sticky-variant-price" class="hlxn-sticky-price  branch-brown">{{ product.price | money_without_trailing_zeros  }}</h4></div>
          {% for product_option in product.options_with_values %}
          {% unless forloop.index == 3 %}
            {% continue %}
          {% endunless %}
      	  
          <div {% unless product_option.values.size > 1 %}style="display:none;"{% endunless %}><h4><strong>{{ product_option.name }}:</strong>&nbsp;<span id="hlxn-sticky-variant-size" class="hlxn-less-weight">{{ product_option.selected_value }}</span></h4></div>
          
          {% endfor %}
          <div class="single-line-header">
          	{% for product_option in product.options_with_values %}
            {% if forloop.index == 3 %}
            {% continue %}
            {% endif %}
            <div>
              <p {% if forloop.index == 2 %}class="space"{% endif %}><strong>{{ product_option.name | remove: " Color" }}</strong>:</p><p id="option-{{ forloop.index }}">{{ product_option.selected_value }}</p>
            </div>
            {% endfor %}
          </div>

          
        </div>
        <div class="btn-side">
          {%- include '_product_buttons', newPdp:true, shipDate: {{product.selected_or_first_available_variant.metafields.variant.shipping}} -%}
        </div>
      </div>
    {% comment %}
    </form>
    {% endcomment %}
  </div>
  <script>
  $(function(){
    // submits the pdp's main form atc button
  	var pntAtcForm = $('#pnt-add-to-cart-form');
    $('#sticky-atc .content-available button').click(function() {
		pntAtcForm.submit()
    });
  });
  </script>
  {% endif %}
<!-- ###############################################################################################  -->
<!-- END STICKY MARKUP  -->
<!-- ###############################################################################################  -->

</div>
{%- include 'specs_sheet_popup' -%}




